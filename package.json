{
  "name": "ExpressjsBlanky",
  "version": "0.1.0",
  "scripts": {
    "test:comment": "# https://github.com/visionmedia/supertest/issues/520 for reason why we use both --forceExit and --detectOpenHandles, ",
    "test:integration": "npx jest --testPathPattern=\".+tests/integration/.+.spec.ts\" --runInBand --detectOpenHandles --forceExit",
    "test:unit": "npx jest --testPathPattern=\".+tests/unit/.+.spec.ts\" --runInBand --detectOpenHandles --forceExit",
    "script": "babel-node --extensions \".ts,.tsx\"",
    "test": "npx jest --runInBand --detectOpenHandles --forceExit --silent",
    "test:setup": "npm run dev:setup",
    "dev:setup": "./bin/dev_setup.sh",
    "dev": "npm run build:copy && npx tsc --watch",
    "dev:debug": "skaffold debug --port-forward",
    "build": "npm run build:compile && npm run build:copy",
    "build:compile": "rm -rf dist && npx tsc",
    "build:copy": "npx copyfiles -u 1 app/.env dist/ && npx copyfiles -u 1 app/Versionfile dist/ && npx copyfiles app/config/* -u 2 dist/config/",
    "db:migrate:create": "npx sequelize migration:create",
    "db:migrate": "npx sequelize db:migrate --debug",
    "db:migrate:undo": "npx sequelize db:migrate:undo",
    "generate:doc": "npx apidoc -i app/ -o docs/",
    "format": "npx prettier --write \"{app,tests}/**/*.ts\"",
    "lint": "npx tsc -b tsconfig.lint.json && npx eslint --fix --ext ts,js ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,css,md}": [
      "npm run format"
    ]
  },
  "author": "Levi Bostian <levi@curiosityio.com>",
  "dependencies": {
    "@godaddy/terminus": "^4.3.1",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "bull": "^3.12.1",
    "bull-arena": "^2.6.4",
    "connect-sequence": "https://github.com/sirap-group/connect-sequence/tarball/dacd6d9925",
    "cors": "^2.8.5",
    "dayjs": "^1.9.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-brute": "^1.0.1",
    "express-brute-redis": "0.0.1",
    "express-mung": "^0.5.1",
    "express-routes-versioning": "^1.0.1",
    "express-validator": "^6.3.0",
    "firebase-admin": "^8.8.0",
    "helmet": "^3.21.2",
    "honeybadger": "^1.3.0",
    "humps": "^2.0.1",
    "lodash.isequal": "^4.5.0",
    "lodash.isnil": "^4.0.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.omitby": "^4.6.0",
    "passport": "^0.4.1",
    "passport-anonymous": "^1.0.1",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^7.14.0",
    "pg-hstore": "^2.3.3",
    "postmark": "^2.3.4",
    "query-string": "^6.13.4",
    "sequelize": "^5.21.2",
    "uid2": "^0.0.3"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.7",
    "@types/bluebird": "^3.5.29",
    "@types/bull": "^3.14.2",
    "@types/bull-arena": "^2.6.0",
    "@types/connect-sequence": "^2.1.0",
    "@types/connect-trim-body": "0.0.0",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/express-brute": "0.0.37",
    "@types/express-mung": "^0.5.2",
    "@types/express-routes-versioning": "^1.0.1",
    "@types/helmet": "0.0.45",
    "@types/honeybadger": "^1.3.1",
    "@types/humps": "^1.1.2",
    "@types/jest": "^26.0.14",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.isnil": "^4.0.6",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/lodash.omitby": "^4.6.6",
    "@types/node": "^12.12.17",
    "@types/passport": "^1.0.2",
    "@types/passport-anonymous": "^1.0.3",
    "@types/passport-http": "^0.3.8",
    "@types/passport-http-bearer": "^1.0.34",
    "@types/query-string": "^6.3.0",
    "@types/redis": "^2.8.14",
    "@types/supertest": "^2.0.8",
    "@types/uid2": "0.0.0",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "copyfiles": "^2.4.0",
    "eslint": "^6.7.2",
    "eslint-plugin-jest": "^23.1.1",
    "husky": "^3.1.0",
    "jest-junit": "^11.0.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "source-map-support": "^0.5.19",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  }
}
