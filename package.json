{
  "name": "ExpressjsBlanky",
  "version": "0.1.0",
  "scripts": {
    "test:integration": "npx jest --testPathPattern=\".+tests/integration/.+.spec.ts\" --runInBand --forceExit",
    "test:unit": "npx jest --testPathPattern=\".+tests/unit/.+.spec.ts\"",
    "test": "npx concurrently --names \"unit, integration\" -c \"blue,green\" \"npm run test:unit\" \"npm run test:integration\"",
    "test:setup": "npm run dev:setup",
    "dev:setup": "npm run db && npm run redis && sleep 5 && npm run db:migrate",
    "dev": "npx concurrently --kill-others --names \"lint,node\" -c \"blue,green\" \"npx tsc --pretty --watch\" \"npx nodemon --watch app --inspect=0.0.0.0 app/app_startup.ts\"",
    "db": "docker-compose -f docker/docker-compose.db.yml up -d",
    "redis": "docker-compose -f docker/docker-compose.redis.yml up -d",
    "db:migrate:create": "npx sequelize migration:create",
    "db:migrate": "npx sequelize db:migrate --debug",
    "db:migrate:undo": "npx sequelize db:migrate:undo",
    "generate:doc": "npx apidoc -i app/ -o api_docs/",
    "format": "npx prettier --write \"{app,tests}/**/*.ts\"",
    "lint": "npx concurrently --names \"ts, eslint\" -c \"blue,green\" \"npx tsc --noEmit\" \"npx eslint --fix --ext ts,js .\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run generate:doc && git add docs && lint-staged && npm run lint"
    }
  },
  "lint-staged": {
    "{app,tests}.{ts,js,json,css,md}": [
      "npm run format",
      "git add"
    ]
  },
  "author": "Levi Bostian <levi@curiosityio.com>",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "bull": "^3.12.1",
    "bull-arena": "^2.6.4",
    "connect-sequence": "https://github.com/sirap-group/connect-sequence/tarball/dacd6d9925",
    "connect-trim-body": "^0.0.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-brute": "^1.0.1",
    "express-brute-redis": "0.0.1",
    "express-routes-versioning": "^1.0.1",
    "express-validator": "^6.3.0",
    "firebase-admin": "^8.8.0",
    "forever": "^1.0.0",
    "helmet": "^3.21.2",
    "honeybadger": "^1.3.0",
    "humps": "^2.0.1",
    "lodash.isplainobject": "^4.0.6",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^7.14.0",
    "pg-hstore": "^2.3.3",
    "postmark": "^2.3.4",
    "sequelize": "^5.21.2",
    "uid2": "^0.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-typescript": "^7.7.4",
    "@types/bluebird": "^3.5.29",
    "@types/bull": "^3.10.6",
    "@types/bull-arena": "^2.6.0",
    "@types/connect-sequence": "^2.1.0",
    "@types/connect-trim-body": "0.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/express-brute": "0.0.37",
    "@types/express-routes-versioning": "^1.0.1",
    "@types/helmet": "0.0.45",
    "@types/honeybadger": "^1.3.1",
    "@types/humps": "^1.1.2",
    "@types/jest": "^24.0.23",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/node": "^12.12.17",
    "@types/passport": "^1.0.2",
    "@types/passport-http": "^0.3.8",
    "@types/passport-http-bearer": "^1.0.34",
    "@types/redis": "^2.8.14",
    "@types/supertest": "^2.0.8",
    "@types/uid2": "0.0.0",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "apidoc": "^0.19.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "concurrently": "^5.0.1",
    "eslint": "^6.7.2",
    "eslint-plugin-jest": "^23.1.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2",
    "typescript": "^3.7.3"
  }
}
