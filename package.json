{
  "name": "ExpressjsBlanky",
  "version": "0.1.0",
  "main": "app/server.js",
  "scripts": {
    "build": "./node_modules/babel-cli/bin/babel.js app --out-dir dist --copy-files",
    "run-build": "./node_modules/forever/bin/forever dist/server.js",
    "test": "DEBUG=*,-babel*,-mocha*,-express*,-ioredis* ./node_modules/mocha/bin/mocha \"test/**/*.js\" -b --exit --retries 1 --timeout 12000 --require babel-core/register",
    "test-ci": "DEBUG=-* ./node_modules/mocha/bin/mocha \"test/**/*.js\" -b --exit --retries 1 --timeout 12000 --require babel-core/register",
    "docker-test-build": "docker-compose -f docker-compose.yml -f docker-compose.test.yml build",
    "docker-test-ci-build": "docker-compose -f docker-compose.yml -f docker-compose.test.yml -f docker-compose.test-ci.yml build",
    "docker-test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up --abort-on-container-exit",
    "docker-test-ci": "docker-compose -f docker-compose.yml -f docker-compose.test.yml -f docker-compose.test-ci.yml up --abort-on-container-exit",
    "start": "DEBUG=*,-babel*,-express*,-ioredis* ./node_modules/babel-watch/babel-watch.js --inspect -L -w app app/server.js",
    "docker-dev-build": "docker-compose -f docker-compose.yml -f docker-compose.dev.override.yml build",
    "docker-dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.override.yml up",
    "doc": "apidoc -i app/ -o apidoc/",
    "test:debug": "DEBUG=*,-babel*,-mocha*,-express*,-ioredis* ./node_modules/mocha/bin/mocha \"test/**/*.js\" -b --exit --retries 1 --timeout 12000 --inspect-brk=0.0.0.0:9229 --require babel-core/register",
    "coverage": "./node_modules/mocha/bin/mocha --require blanket -R html-cov --compilers js:babel-core/register > coverage.html",
    "db-migrate-create": "./node_modules/.bin/sequelize migration:create",
    "db-migrate-run": "./node_modules/.bin/sequelize db:migrate",
    "db-migrate-undo": "./node_modules/.bin/sequelize db:migrate:undo",
    "lint": "./node_modules/.bin/eslint *.js app/**/*.js test/**/*.js",
    "lint-fix": "./node_modules/.bin/eslint --fix *.js app/**/*.js test/**/*.js",
    "flow-type-install": "./node_modules/.bin/flow-typed install"
  },
  "config": {
    "blanket": {
      "pattern": "app/controllers/"
    }
  },
  "author": "Levi Bostian <levi@curiosityio.com>",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "body-parser": "^1.18.2",
    "bull": "^3.3.8",
    "bull-arena": "^2.4.2",
    "connect-trim-body": "^0.0.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-routes-versioning": "^1.0.1",
    "express-validator": "^5.2.0",
    "firebase-admin": "^5.12.1",
    "flow-bin": "^0.74.0",
    "forever": "^0.15.3",
    "helmet": "^3.10.0",
    "mailgun-js": "^0.18.1",
    "mustache": "^2.2.1",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.0.0",
    "uid2": "^0.0.3",
    "winston": "^3.0.0",
    "winston-slacker": "^1.0.3"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "babel-eslint": "^8.2.5",
    "babel-watch": "^2.0.7",
    "blanket": "^1.2.3",
    "chai": "^4.1.2",
    "eslint": "^4.16.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.13.0",
    "mocha": "^4.1.0",
    "nock": "^9.3.3",
    "should": "^13.2.1",
    "supertest": "^3.0.0"
  }
}
