{
  "name": "ExpressjsBlanky",
  "version": "0.1.0",
  "description": "Blank API project to get up and running FAST.",
  "main": "app/server.js",
  "scripts": {
    "clean": "rm -rf db.*.sqlite",
    "build": "./node_modules/babel-cli/bin/babel.js app --out-dir dist --copy-files",
    "run-build": "./node_modules/forever/bin/forever dist/server.js",
    "test": "DEBUG=nock.* VIRTUAL_HOST=foo.levibostian.com API_MAILGUN_API_KEY=fake ./node_modules/mocha/bin/mocha \"test/**/*.js\" -b --timeout 2000 --compilers js:babel-core/register",
    "start": "./node_modules/babel-watch/babel-watch.js app/server.js",
    "doc": "apidoc -i app/ -o apidoc/",
    "debug-step1": "node-debug app/server.js",
    "debug-step2": "./node_modules/babel-cli/bin/babel-node.js --debug app/server.js",
    "testdebug": "./node_modules/mocha/bin/mocha --debug-brk --compilers js:babel-core/register",
    "coverage": "./node_modules/mocha/bin/mocha --require blanket -R html-cov --compilers js:babel-core/register > coverage.html",
    "db-migrate-create": "./node_modules/.bin/sequelize migration:create",
    "db-migrate-run": "./node_modules/.bin/sequelize db:migrate",
    "db-migrate-undo": "./node_modules/.bin/sequelize db:migrate:undo",
    "lint": "./node_modules/.bin/eslint app/**/*.js",
    "lint-fix": "./node_modules/.bin/eslint --fix app/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/levibostian/ExpressjsBlanky"
  },
  "config": {
    "blanket": {
      "pattern": "app/controllers/"
    }
  },
  "author": "Levi Bostian <levi@curiosityio.com>",
  "dependencies": {
    "async": "2.5.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.13.2",
    "connect-trim-body": "0.0.3",
    "express": "^4.12.3",
    "express-validator": "4.1.1",
    "helmet": "3.8.1",
    "mailgun-js": "0.13.1",
    "mustache": "^2.2.1",
    "passport": "0.4.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "7.3.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.5.0",
    "sequelize-cli": "3.0.0-3",
    "uid2": "0.0.3",
    "winston": "^2.2.0",
    "winston-slacker": "^1.0.3"
  },
  "devDependencies": {
    "apidoc": "0.17.6",
    "babel-eslint": "^8.0.0",
    "babel-watch": "^2.0.7",
    "blanket": "^1.2.3",
    "chai": "4.1.2",
    "eslint": "^4.7.0",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "3.5.3",
    "nock": "^9.0.14",
    "nodemon": "^1.12.1",
    "should": "13.0.1",
    "supertest": "3.0.0"
  }
}
