{
  "name": "ExpressjsBlanky",
  "version": "0.1.0",
  "scripts": {
    "test:comment": "# https://github.com/visionmedia/supertest/issues/520 for reason why we use both --forceExit and --detectOpenHandles, ",
    "test:integration": "./bin/tests.sh integration",
    "test:unit": "./bin/tests.sh unit",
    "test": "./bin/tests.sh",
    "dev:setup": "./bin/dev_setup.sh",
    "dev": "npm run build:copy && npx tsc --watch",
    "dev:debug": "skaffold debug --port-forward",
    "build": "rm -rf dist && npx tsc",
    "db:migrate:create": "npx sequelize migration:create",
    "db:migrate": "npx sequelize db:migrate --debug",
    "db:migrate:undo": "npx sequelize db:migrate:undo",
    "generate:doc": "npx apidoc -i app/ -o docs/",
    "format": "npx prettier --write \"{app,tests}/**/*.ts\"",
    "lint": "npx eslint --fix --ext ts,js . && npx tsc -b tsconfig.lint.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,css,md}": [
      "npm run format"
    ]
  },
  "author": "Levi Bostian <levi@curiosityio.com>",
  "subsets": {
    "test": {
      "exclude": [
        "nodemon",
        "eslint",
        "eslint-plugin-jest",
        "husky",
        "lint-staged",
        "prettier"
      ]
    },
    "dev_container": {
      "exclude": [
        "codecov",
        "jest",
        "jest-junit",
        "eslint",
        "eslint-plugin-jest",
        "husky",
        "lint-staged",
        "prettier",
        "ts-jest",
        "supertest"
      ]
    },
    "build": {
      "exclude": [
        "codecov",
        "nodemon",
        "jest",
        "jest-junit",
        "eslint",
        "eslint-plugin-jest",
        "husky",
        "lint-staged",
        "prettier",
        "ts-jest",
        "supertest"
      ]
    }
  },
  "dependencies": {
    "@godaddy/terminus": "^4.3.1",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "bull": "^3.18.0",
    "bull-arena": "^3.2.3",
    "connect-sequence": "https://github.com/sirap-group/connect-sequence/tarball/dacd6d9925",
    "cors": "^2.8.5",
    "dayjs": "^1.9.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-brute": "^1.0.1",
    "express-brute-redis": "0.0.1",
    "express-mung": "^0.5.1",
    "express-routes-versioning": "^1.0.1",
    "express-validator": "^6.6.1",
    "firebase-admin": "^9.2.0",
    "helmet": "^4.1.1",
    "honeybadger": "^1.4.0",
    "humps": "^2.0.1",
    "lodash.isequal": "^4.5.0",
    "lodash.isnil": "^4.0.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.omitby": "^4.6.0",
    "passport": "^0.4.1",
    "passport-anonymous": "^1.0.1",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^8.4.1",
    "pg-hstore": "^2.3.3",
    "postmark": "^2.6.0",
    "query-string": "^6.13.6",
    "sequelize": "^6.3.5",
    "uid2": "^0.0.3"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.7",
    "@types/bull": "^3.14.4",
    "@types/bull-arena": "^3.0.1",
    "@types/connect-sequence": "^2.1.0",
    "@types/connect-trim-body": "0.0.0",
    "@types/cors": "^2.8.8",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/express-brute": "^1.0.0",
    "@types/express-mung": "^0.5.2",
    "@types/express-routes-versioning": "^1.0.1",
    "@types/honeybadger": "^1.3.1",
    "@types/humps": "^2.0.0",
    "@types/jest": "^26.0.15",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.isnil": "^4.0.6",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/lodash.omitby": "^4.6.6",
    "@types/node": "^14.14.2",
    "@types/passport": "^1.0.4",
    "@types/passport-anonymous": "^1.0.3",
    "@types/passport-http": "^0.3.8",
    "@types/passport-http-bearer": "^1.0.36",
    "@types/query-string": "^6.3.0",
    "@types/redis": "^2.8.28",
    "@types/supertest": "^2.0.10",
    "@types/uid2": "0.0.0",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "sequelize-cli": "^6.2.0",
    "source-map-support": "^0.5.19",
    "typescript": "^4.0.3"
  }
}